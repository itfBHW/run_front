{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "chrome",
          "request": "launch",
          "name": "vuejs: chrome",
          "url": "http://localhost:8080",
          "webRoot": "${workspaceFolder}/run_front/src",
          "breakOnLoad": true,
          "sourceMapPathOverrides": {
              "webpack:///src/*": "${webRoot}/*",
              "webpack:///./src/*": "${webRoot}/*"
          }
      }
  ]
}